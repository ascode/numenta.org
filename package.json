{
  "name": "numenta.org",
  "version": "0.0.1",
  "description": "Numenta.org NuPIC Website",
  "scripts": {
    "build": "npm install && npm run guide && npm run generate",
    "check": "gulp check",
    "clean": "rm -Rf _site/*",
    "dev": "npm install && npm run guide && npm run serve",
    "generate": "bundle exec jekyll build",
    "guide": "kss-node --config styleguide/_config/config.json",
    "serve": "bundle exec jekyll serve --baseurl ''",
    "test": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numenta/numenta.org.git"
  },
  "keywords": [
    "NuPIC",
    "Numenta",
    "Platform",
    "Intelligent",
    "Computing",
    "Machine",
    "Intelligence",
    "Hierarchical",
    "Temporal",
    "Memory",
    "HTM"
  ],
  "author": "Numenta",
  "bugs": {
    "url": "https://github.com/numenta/numenta.org/issues"
  },
  "homepage": "http://numenta.org",
  "dependencies": {
    "bootstrap-sass": "3.3.4",
    "font-awesome": "4.3.0",
    "jquery": "2.1.3",
    "open-sands": "1.0.3",
    "platform": "1.3.0"
  },
  "devDependencies": {
    "check-pages": "0.8.0",
    "gulp": "3.8.11",
    "gulp-webserver": "0.9.0",
    "kss": "2.0.2"
  }
}
